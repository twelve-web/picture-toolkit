    "toolkit",
    "vue",
    "react",
    "typescript"
  ],
  "author": "twelve-web",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/twelve-web/picture-toolkit"
  },
  "devDependencies": {
    "tsup": "^8.3.6",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "vue": ">=3.0.0"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
