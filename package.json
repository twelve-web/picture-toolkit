{
  "name": "picture-toolkit",
  "version": "1.0.4",
  "description": "A toolkit for image processing with Vue and React support",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup index.ts --format cjs,esm --dts",
